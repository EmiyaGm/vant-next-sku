{
  "git": {
    "tagName": "v${version}",
    "commitMessage": "chore: release ${version}"
  },
  "npm": {
    "skipChecks": true
  },
  "github": {
    "release": true
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  }
}